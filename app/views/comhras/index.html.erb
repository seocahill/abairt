<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <div class="min-w-full" >
    <div data-controller="map" data-map-comhras-value="<%= @comhras.to_json %>" id="map" style="height:600px"></div>
  </div>
  <br>
  <div class="flex justify-between items-center">
    <%= link_to 'New comhra', new_comhra_path, class: "rounded-lg py-3 px-5 bg-blue-600 text-white block font-medium" %>
  </div>
</div>